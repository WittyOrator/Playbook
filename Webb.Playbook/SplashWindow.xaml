    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="SplashWindow" Height="300" Width="500"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    BorderThickness="0"
    BorderBrush="Transparent">
    <Grid x:Name="grid">
        <Image Margin="0" Source="/Playbook;component/Resource/Splash.png" Stretch="Fill">
            
        </Image>
        <Image x:Name="ball" Source="/Playbook;component/Resource/Football.png" Margin="228,0,228,87" Height="22" VerticalAlignment="Bottom">
            
        </Image>
        <Image Margin="0,0,32,12" Name="image1" Stretch="Fill" Source="/Playbook;component/Resource/logo.PNG" Height="42" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="153" Opacity="0.5" />
    </Grid>
</Window>
