<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	x:Class="Webb.Playbook.Call.NewCall"
	x:Name="Window"
	Title="NewCall"
	Width="580" Height="560" Loaded="Window_Loaded">

    <Grid x:Name="GridCall">
        <Grid.RowDefinitions>
            <RowDefinition Height="0.898*"/>
            <RowDefinition Height="32"/>
        </Grid.RowDefinitions>
        <TabControl x:Name="tabCall"   Width="Auto" Height="Auto" Margin="8,8,8,0">

        </TabControl>
        <Button HorizontalAlignment="Left" x:Name="btnOk"   Margin="113,4,0,2" Width="104"  Height="26" Content="OK" Grid.Row="1"/>
        <Button HorizontalAlignment="Right" x:Name="btnCancel" Margin="0,4,111,2" Width="104" Height="26" Content="Cancel" Grid.Row="1"/>
    </Grid>
</Window>